{# This is just a layout example for the styleguide.
  Do not use this template directly. #}

{% set styles = [] %}
{% set styles = gap ? styles|merge(['--gap: var(' ~ gap ~ ');']) : styles %}
{% set styles = measure ? styles|merge(['--measure: ' ~ measure ~ ';']) : styles %}

{% set classes = [
  'switcher',
  'switcher-demo',
] %}

<div class="{{ classes|join(' ') }}" style="{{ styles|join }}">
	{% for i in range(1, num_items) %}
		<div class="box">{{ i }}</div>
	{% endfor %}
</div>
